{
  "name": "react-invoicing",
  "version": "1.0.0",
  "description": "Create free and unlimited one off invoices using this little Invoicing app",
  "main": "index.js",
  "watch" : {
    "bundle": "{js,bundle}/main.js"
  },
  "scripts": {
    "bundle": "browserify -t [ babelify --presets [ @babel/preset-env @babel/preset-react ] ] ./js/main.js -o ./js/bundle.js",
    "watch" : "npm-watch",
    "serve" : "lite-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emanu-dev/react-invoicing.git"
  },
  "author": "Emanuel Prado",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emanu-dev/react-invoicing/issues"
  },
  "homepage": "https://github.com/emanu-dev/react-invoicing#readme",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "commitizen": "^4.2.3",
    "create-react-class": "^15.7.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "angular": "^1.5.3",
    "eslint": "^7.22.0",
    "npm-watch": "^0.9.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react2angular": "^4.0.6"
  }
}
